<T3DataStructure>
	<meta>
	    <langDisable>1</langDisable>
	</meta>
	  <ROOT>
	    <type>array</type>
	    <el>
	      <templateFile>
	      <TCEforms>
	      	<label>Fluid Template File</label>
	      	<config>
		      	<type>group</type>
		      	<internal_type>file</internal_type>
		      	<allowed>html</allowed>
		      	<minitems>0</minitems>
		      	<maxitems>1</maxitems>
		      	<size>1</size>
	      	</config>
	      </TCEforms>
	      </templateFile>
	      <templateSource>
	      	<TCEforms>
	      	<label>Fluid Template Source Code</label>
	      	<config>
		      	<type>text</type>
		      	<rows>120</rows>
		      	<cols>100</cols>
	      	</config>
	      	</TCEforms>
	      </templateSource>
	      <fluidVars>
	      	<TCEforms>
	      	<label>Custom Fluid Variables (in proper JSON format!)</label>
	      	<config>
		      	<type>text</type>
		      	<rows>120</rows>
		      	<cols>100</cols>
	      	</config>
	      	</TCEforms>
	      </fluidVars>
	    </el>
	  </ROOT>
</T3DataStructure>